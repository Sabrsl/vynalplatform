{
  "name": "vynal-platform",
  "version": "0.1.138",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=development next dev",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "start:prod": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "check:all": "npm run lint && npm run typecheck",
    "clear": "if exist .next rmdir /s /q .next && npm run dev",
    "dev:clean": "rmdir /s /q .next && rmdir /s /q node_modules\\.cache && npm run dev",
    "update-types": "npx supabase gen types typescript --project-id cybeqoprsggfgippxtvt --schema public > src/types/database.ts",
    "find:deadcode": "npx ts-prune",
    "security:audit": "npm audit --production --audit-level=high",
    "prepare": "cross-env-shell \"[ -f .git/hooks/pre-commit ] && rm .git/hooks/pre-commit || true\" && npx husky install",
    "test:email": "node src/scripts/test-email.js",
    "db:migrate:messaging": "npx supabase db push --db-url $NEXT_PUBLIC_SUPABASE_URL --auth-token $SUPABASE_SERVICE_ROLE_KEY --migration-file ./migrations/add_increment_unread_count.sql",
    "db:migrate:permissions": "npx supabase db push --db-url $NEXT_PUBLIC_SUPABASE_URL --auth-token $SUPABASE_SERVICE_ROLE_KEY --migration-file ./migrations/fix_conversation_permissions.sql",
    "validate:services": "node scripts/validate-services.js",
    "update-version": "node scripts/update-version.js",
    "copy:templates": "node scripts/copy-email-templates.js",
    "prebuild": "npm run validate:services && npm run update-version && npm run copy:templates"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
